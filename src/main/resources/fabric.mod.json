{
  "schemaVersion": 1,
  "id": "sessionlogin",
  "version": "${version}",

  "name": "Session Login",
  "description": "Allows you to login from a session id.",
  "authors": [
    "Coderx_Gamer"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/sessionlogin/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "net.sessionlogin.MainClient"
    ]
  },
  "mixins": [
    "sessionlogin.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
